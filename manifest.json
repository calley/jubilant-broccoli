{
"update_url": "https://clients2.google.com/service/update2/crx",

   "manifest_version": 2,
   "browser_action": {
     "default_icon": "photos/face.png",
     "default_title": "Pandora Enhancer",
     "default_popup": "html/popup.html"
   },
   "options_page": "html/options.html",
   "name": "Pandora Enhancer",
   "version": "4.19",
   "version_name": "4.1",
   "description": "Enhances the Pandora Experience with several features",
   "icons": {
      "128": "photos/icon128.png",
      "16": "photos/icon16.png",
      "48": "photos/icon48.png"
   },
   "background": {
   		"scripts": ["js/jquery.js","js/background.js","js/downloader.js","js/ad/prevent.js", "js/browser-id3-writer.js"]
   },
   "content_scripts" : [
     {
       "matches" : ["http://*.pandora.com/*", "https://*.pandora.com/*"],
	   "css": ["css/buttonStyler.css"],
	   "js" : ["js/jquery.js","js/main.js","js/error.js","js/in-page-dl-button.js"]
     }
   ],
   "permissions": ["notifications","tabs","*://*/*", "*://*.p-cdn.com/*", "webRequest", "webRequestBlocking", "activeTab", "downloads"],
   "web_accessible_resources": [ "photos/*","html/downloadButton.html", "js/*" ]
}